
<div class="row" style="align-items:center">
    <div class="col-auto">
        <h1>@Title</h1>
    </div>
    <div class="flex-fill"></div>
    <div class="col-auto">
        @ChildContent
    </div>
</div>

@code {
    [Parameter]
    public string Title { get; set; }


    [Parameter]
    public RenderFragment ChildContent { get; set; }

}
